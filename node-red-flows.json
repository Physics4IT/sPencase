    {
        "id": "65eaa6ca499a4eb5",
        "type": "function",
        "z": "2db4c2eb33a80e37",
        "name": "convert to JS Object",
        "func": "switch (msg.topic) {\n    case \"pub/potentiometer\":\n        global.set(\"brightness\", msg.payload.value);\n        break;\n    \n    case \"pub/button\":\n        global.set(\"buttonState\", msg.payload.value);\n        break;\n\n    case \"pub/photoresistor\":\n        global.set(\"envLightRes\", msg.payload.value);\n        break;\n\n    case \"pub/dht\":\n        {\n            if (\"temperature\" in msg.payload) \n                global.set(\"temperature\", msg.payload.temperature);\n            if (\"humidity\" in msg.payload)\n                global.set(\"humidity\", msg.payload.humidity);\n        }\n        break;\n\n    case \"pub/uvSensor\":\n        global.set(\"uv\", msg.payload.value);\n        break;\n\n    case \"pub/tiltSensor\":\n        global.set(\"tilt\", msg.payload.value);\n        break;\n\n    case \"pub/ultrasonicSensor\":\n        global.set(\"distance\", msg.payload.value);\n        break;\n    \n    default:\n        break;\n}\n\nmsg.payload = Object({\n    \"brightness\": global.get(\"brightness\"),\n    \"buttonState\": global.get(\"buttonState\"),\n    \"envLightRes\": global.get(\"envLightRes\"),\n    \"temperature\": global.get(\"temperature\"),\n    \"humidity\": global.get(\"humidity\"),\n    \"uv\": global.get(\"uv\"),\n    \"tilt\": global.get(\"tilt\"),\n    \"distance\": global.get(\"distance\"),\n});\n\nreturn msg;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "// Code added here will be run once\n// whenever the node is started.\nglobal.set(\"potentioValue\", 0);\nglobal.set(\"buttonState\", 0); // 1: Clicked, 0: Not clicked\nglobal.set(\"brightness\", 0);\nglobal.set(\"temperature\", 0);\nglobal.set(\"humidity\", 0);\nglobal.set(\"uv\", 0);\nglobal.set(\"tilt\", 0);\nglobal.set(\"distance\", 0);",
        "finalize": "",
        "libs": [],
